<?xml version="1.0"?>
<display:FileHistory xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:mx="library://ns.adobe.com/flex/mx"
        xmlns:display="com.vango.testing.performance.viewer.controls.display.*"
        width="100%">
    <s:HGroup width="100%">
        <s:DropDownList id="dropDown" dataProvider="{history}" labelField="path" selectedItem="{selectedEntry}" width="100%"/>
        <s:Button id="button" toolTip="Browse..." click="onBrowseClick(event)" skinClass="com.vango.testing.performance.viewer.controls.display.skins.SmallButtonSkin"/>
        <s:Button id="clear" toolTip="Clear..." click="onClearClick(event)" skinClass="com.vango.testing.performance.viewer.controls.display.skins.SmallButtonSkin"/>
    </s:HGroup>
</display:FileHistory>
